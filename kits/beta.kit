              <form name="beta-form" id="beta-form" class="signup-form horizontal-form" action="bin/beta.php" method="post" enctype="text/plain" autocomplete="off" novalidate>
                  <div class="control-group col-sm-* col-md-5">
                      <label class="control-label form-q" for="name">What's your name?</label>
                      <div class="controls">
