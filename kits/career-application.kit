    <body class="fm fm-career" id="career">    

       <div class="container">
          <div class="marketing" id="section0">      

            <div class="fs-title row">
              <h1>Career application</h1>                    
            </div> 
        
            <form name="career-form" class="signup-form" action="MAILTO:beta@jonar.com" method="post" enctype="text/plain" id="career-form" autocomplete="off" novalidate>
              
              <div class="control-group col-lg-*">
                  <label class="control-label form-q" for="name">What's your name?</label>
                <div class="controls">
                  <input class="form-a" id="name" name="name" type="text" placeholder="Your name" data-validation-required-message="Please enter your name" required/>               
                <p class="help-block"></p>
                </div>
              </div>
              
              <div class="control-group col-lg-*">
                  <label class="control-label form-q" for="number" >What's your phone number?</label>
                <div class="controls">
                  <input class="form-a" type="text" name="number" id="number" placeholder="Contact number" data-validation-required-message="Please enter your number" required />
                  <p class="help-block"></p>
                </div>
              </div>
                  
              <div class="control-group col-lg-*">
                  <label class="control-label form-q" for="email">What's your email address?</label>
                <div class="controls">
                  <input class="form-a" type="email" name="email" id="email" data-validation-email-message="Must be a valid email" placeholder="Your email" data-validation-required-message="Please enter your email" required /> 
                  <p class="help-block"></p> 
                </div>              
              </div>
                  
              
              <div class="control-group">
                  <input type="checkbox" class="checkbox" id="notify-me">
                  <label class="lead" for="notify-me">Send me updates</label>
              </div>
              
              <div class="control-group">
                  <label class="control-label form-q">Please upload a copy of your cover letter and CV to the email before it is sent.</label>
              </div>  

             
              <div class="control-group col-sm-*">      
                <button class="form-submit col-sm-2 col-sm-offset-8" type="submit" name="submit" id="submit">Submit</button>
              </div>
            </form>
        </div>           
    </div>     

